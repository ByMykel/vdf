{
  "name": "@node-steam/vdf",
  "version": "2.0.1",
  "description": "Module to convert Valve's KeyValue format to JSON and back",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "homepage": "https://github.com/node-steam/vdf",
  "bugs": "https://github.com/node-steam/vdf/issues",
  "repository": {
    "url": "https://github.com/node-steam/vdf",
    "type": "git"
  },
  "author": "N|Steam",
  "keywords": [
    "steam",
    "valve",
    "vdf",
    "typescript",
    "es6",
    "es7"
  ],
  "files": [
    "lib/*.js",
    "lib/*.d.ts",
    "example/*.js"
  ],
  "contributors": [
    "Silas Rech <silas.rech@protonmail.com> (https://lenovouser.me)"
  ],
  "runkitExampleFilename": "example/runkit.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "license": "MIT",
  "scripts": {
    "compile": "tsc -p .",
    "test": "yarn compile && ava",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "docs": "typedoc src/index.ts",
    "prepublish": "yarn lint && yarn test && yarn docs"
  },
  "ava": {
    "files": [
      "test/*.js",
      "test/**/*.js"
    ],
    "source": [
      "lib/*.js"
    ],
    "concurrency": 5,
    "verbose": true,
    "failFast": true,
    "failWithoutAssertions": false,
    "powerAssert": false,
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "devDependencies": {
    "@types/node": "^8.0.53",
    "app-module-path": "^2.2.0",
    "ava": "^0.24.0",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "typescript": "^2.6.1"
  }
}
